{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/server.ts"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/su-per-man/github-readme-title"
    }
  ]
}
